{
  "i18n": true,
  "type": "panel",
  "items": {
    "interval": {
      "type": "number",
      "label": {
        "en": "Polling interval (minutes)",
        "de": "Polling-Intervall (Minuten)"
      },
      "min": 1,
      "max": 1440,
      "default": 5
    },
    "stations": {
      "type": "table",
      "label": {
        "en": "Stations",
        "de": "Stationen"
      },
      "items": [
        {
          "type": "checkbox",
          "attr": "enabled",
          "title": {
            "en": "Enabled",
            "de": "Aktiv"
          },
          "width": 90,
          "default": true
        },
        {
          "type": "text",
          "attr": "city",
          "title": {
            "en": "City / Location",
            "de": "Ort"
          },
          "width": 180
        },
        {
          "type": "text",
          "attr": "name",
          "title": {
            "en": "Name",
            "de": "Name"
          },
          "width": 220
        },
        {
          "type": "number",
          "attr": "deviceId",
          "title": {
            "en": "Device ID",
            "de": "Device ID"
          },
          "width": 160
        },
        {
          "type": "text",
          "attr": "id",
          "title": {
            "en": "ID (internal)",
            "de": "ID (intern)"
          },
          "width": 140
        }
      ],
      "addRow": true,
      "deleteRow": true
    }
  }
}
