{
  "i18n": true,
  "type": "panel",
  "items": {
    "interval": {
      "type": "number",
      "label": {
        "en": "Polling interval (minutes)",
        "de": "Polling-Intervall (Minuten)"
      },
      "min": 1,
      "max": 1440,
      "default": 5
    },
    "stations": {
      "type": "table",
      "label": {
        "en": "Stations",
        "de": "Stationen"
      },
      "items": [
        {
          "type": "checkbox",
          "attr": "enabled",
          "title": {
            "en": "Enabled",
            "de": "Aktiv"
          },
          "width": 80,
          "default": true
        },
        {
          "type": "text",
          "attr": "name",
          "title": {
            "en": "Name",
            "de": "Name"
          },
          "width": 260
        },
        {
          "type": "number",
          "attr": "deviceId1",
          "title": {
            "en": "Device ID (P1)",
            "de": "Device ID (P1)"
          },
          "width": 170
        },
        {
          "type": "number",
          "attr": "deviceId2",
          "title": {
            "en": "Device ID (P2, optional)",
            "de": "Device ID (P2, optional)"
          },
          "width": 190
        }
      ],
      "addRow": true,
      "deleteRow": true
    }
  }
}
